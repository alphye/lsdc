<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="lsdc-kernel-service"  organization="lszn" />

    <dubbo:protocol name="dubbo" port="${lsdc.dubbo.port_service}" />

    <dubbo:registry protocol="zookeeper" address="${lsdc.dubbo.zookeeper_address}" client="zkclient" register="false" />

</beans>
